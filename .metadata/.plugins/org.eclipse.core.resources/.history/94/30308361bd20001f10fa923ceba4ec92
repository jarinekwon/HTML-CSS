import java.util.Scanner;

public class ExMaxMinP {
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		System.out.print("정수 3개 입력>>");
		int a = scan.nextInt();
		int b = scan.nextInt();
		int c = scan.nextInt();
		int max;
		int min;
		
		if (a > b && a > c) {
			max = a;
		}
		else if (b > a && b > c) {
			max = b;
		}
		else if (c > a && c > b) {
			max = c;
		}
		
		if (a < b && a < c) {
			min = a;
		}
		else if (b < a && b < c) {
			min = b;
		}
		else if (c < a && c < b) {
			min = c;
		}
		
		System.out.println("최대값은 " + max);
		System.out.println("최소값은 " + min);
	}
}
