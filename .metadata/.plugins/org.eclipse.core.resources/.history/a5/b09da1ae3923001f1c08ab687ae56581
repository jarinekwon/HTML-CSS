import java.util.Scanner;

public class ArithmeticExSwitchP {
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		System.out.print("연산>>");
		int num1 = scan.nextInt();
		String s = scan.next();
		int num2 = scan.nextInt();
		int res = 0;
		
		switch (s) {
		case "+":
			res = num1 + num2;
			break;
		case "-":
			res = num1 - num2;
			break;
		case "*":
			res = num1 * num2;
			break;
		case "/":
			if (num2 == 0) {
				System.out.println("0으로 나눌 수 없습니다.");
				return;
			} else {
				res = num1 / num2;
				return;
			}
			break;

		default:
			break;
		}
	}
}
